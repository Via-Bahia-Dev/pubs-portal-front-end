<!DOCTYPE html>
<html>
<head>
  <title>PubsPortal</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if flash[:error] %>
	<p> <%= flash[:error] %> </p>
<% end %>
<% if flash[:success] %>
	<p> <%= flash[:success] %> </p>
<% end %>

<%= yield %>

</body>
</html>
